<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="JC_Web_System" extends="struts-default">
		<action name="userLogin" class="userLoginAction">
			<result name="LoginSuccess">/Pages/index.jsp</result>
			<result name="LoginFail">/login.jsp</result>
			<result name="error">/Pages/error.jsp</result>
			<result name="LogOut">/login.jsp</result>
		</action>
		<action name="userList" class="userListAction">
			<result name="userList">/Pages/user_List.jsp</result>
			<result name="userAdd">/Pages/user_AddEdit.jsp</result>
			<result name="userAddSuccess">/Pages/user_List.jsp</result>
		</action>
	</package>
</struts>    
